columnlines <- c("black", "red", "blue")
labels <- c("Sub_metering_1", "Sub_metering_2", "Sub_metering_3")
plot(finaldata$Time, finaldata$Sub_metering_1, type="l", col=columnlines[1], xlab="", ylab="Energy sub metering")
lines(finaldata$Time, finaldata$Sub_metering_2, col=columnlines[2])
lines(finaldata$Time, finaldata$Sub_metering_3, col=columnlines[3])
legend("topright", legend=labels, col=columnlines, lty="solid")
dev.copy(png,"plot3.png", width=480, height=480)
dev.off()
